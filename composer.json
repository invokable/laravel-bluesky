{
  "name": "revolution/laravel-bluesky",
  "description": "Laravel Bluesky",
  "keywords": [
    "laravel",
    "bluesky",
    "atproto",
    "socialite",
    "notifications"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-pcntl": "*",
    "illuminate/support": "^11.20",
    "laravel/socialite": "^5.16",
    "guzzlehttp/guzzle": "^7.8",
    "firebase/php-jwt": "^6.10",
    "phpseclib/phpseclib": "^3.0",
    "revolution/atproto-lexicon-contracts": "^1.0.25",
    "valtzu/guzzle-websocket-middleware": "^0.2.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\Bluesky\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\Bluesky\\Providers\\BlueskyServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
